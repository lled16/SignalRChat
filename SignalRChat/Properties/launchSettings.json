{
  "profiles": {
    "SignalRChat": {
      "commandName": "Project",
      "launchUrl": "http://*:5001",
      "applicationUrl": "http://*:5001"
    },
    "Container (Dockerfile)": {
      "commandName": "Docker",
      "launchUrl": "http://*:5001",
      "environmentVariables": {
        "ASPNETCORE_HTTP_PORTS": "5001"
      },
      "publishAllPorts": true
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}